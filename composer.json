{
  "require": {
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4",
    "symfony/dotenv": "^4",
    "symfony/yaml": "^4",
    "symfony/orm-pack": "^1.0",
    "symfony/phpunit-bridge": "^4"
  },
  "require-dev": {
    "symfony/profiler-pack": "^1.0",
    "symfony/web-server-bundle": "^4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
